{% extends 'AdminBundle:Setup:setup.base.html.twig' %}

{% block header_left %}
    <a href="{{ path('_admin_index') }}" data-role="button" data-icon="home" data-iconpos="notext" data-mini="{{ jquery_mobile_theme_mini }}" data-iconshadow="true" class="ui-btn-left" >{% trans %}header.left.nav.home{% endtrans %}</a>
{% endblock %}

{% block content %}
    <ul data-role="listview" data-inset="true" data-theme="c">
        <li class"ui-btn">
            <a href="{{ path('_prospects_index') }}" data-transition="slide">
                {% trans %}contacts.type.prospect.title{% endtrans %}
            </a>
        </li>
        <li class"ui-btn">
            <a href="#" data-transition="slide">
                {% trans %}contacts.type.customer.title{% endtrans %}
            </a>
        </li>
    </ul>
{% endblock %}


